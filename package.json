{
  "name": "ogma",
  "version": "0.0.0",
  "private": true,
  "description": "The @ogma workspace containing the base logging package, the NestJS module, and all the fun parsers to add in.",
  "author": "Jay McDoniel <me@jaymcdoniel.dev>",
  "contributors": [
    "Minsung Kim",
    "John Biundo",
    "Ruslan Gonzalez"
  ],
  "workspaces": [
    "packages/*"
  ],
  "keywords": [
    "nestjs",
    "nest",
    "logger",
    "json",
    "json-logging",
    "request-logging",
    "nestjs-module",
    "module",
    "logging",
    "microservice",
    "http",
    "gql",
    "graphql",
    "websocket",
    "ws"
  ],
  "scripts": {
    "preversion": "pnpm format && pnpm lint && pnpm build",
    "build": "nx affected:build",
    "benchmark": "pnpm run start --filter '@ogma/bench*'",
    "commit": "git-cz",
    "precoverage": "tsc -p ./tools/tsconfig.json",
    "coverage": "node ./tools/coverage-merge",
    "postcoverage": "nyc merge coverage-tmp coverage/coverage-final.json && nyc report -t coverage --report-dir coverage --reporter=lcov",
    "format": "prettier --write \"{packages,integration,benchmarks,tools}/**/*.ts\"",
    "lint": "eslint --ext .ts .",
    "test": "pnpm run test --filter='!@ogma/integration'",
    "test:cov": "pnpm run test:cov --filter=\"!@ogma/integration\"",
    "test:int": "pnpm run test --filter @ogma/integration",
    "test:debug": "node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand",
    "updatePeerDeps": "node tools/peer-update.js",
    "sendCoverage": "codeclimate-test-reporter < coverage/lcov.info",
    "prepare": "husky install",
    "release": "pnpm build && changeset publish",
    "nx": "nx"
  },
  "devDependencies": {
    "@changesets/cli": "^2.16.0",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@golevelup/nestjs-modules": "^0.4.2",
    "@golevelup/ts-jest": "^0.3.1",
    "@grpc/proto-loader": "^0.6.2",
    "@nestjs/common": "^7.6.18",
    "@nestjs/core": "^7.6.18",
    "@nestjs/graphql": "^7.11.0",
    "@nestjs/microservices": "^7.6.18",
    "@nestjs/platform-express": "^7.6.18",
    "@nestjs/platform-fastify": "^7.6.18",
    "@nestjs/platform-socket.io": "^7.6.18",
    "@nestjs/platform-ws": "^7.6.18",
    "@nestjs/testing": "^7.6.18",
    "@nestjs/websockets": "^7.6.18",
    "@nrwl/cli": "latest",
    "@nrwl/eslint-plugin-nx": "12.4.0",
    "@nrwl/jest": "12.4.0",
    "@nrwl/linter": "12.4.0",
    "@nrwl/nest": "^12.4.0",
    "@nrwl/node": "^12.4.0",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/tao": "latest",
    "@nrwl/workspace": "latest",
    "@types/bunyan": "^1.8.6",
    "@types/express": "^4.17.12",
    "@types/express-serve-static-core": "^4.17.21",
    "@types/jest": "26.0.8",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "amqp-connection-manager": "^3.2.2",
    "amqplib": "^0.8.0",
    "apollo-server-express": "^2.25.1",
    "apollo-server-fastify": "^2.25.1",
    "bunyan": "^1.8.15",
    "codeclimate-test-reporter": "^0.5.1",
    "conventional-changelog-cli": "^2.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "express": "^4.17.1",
    "fastify": "^3.18.0",
    "graphql": "^14",
    "graphql-tools": "^7.0.5",
    "grpc": "^1.24.10",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "kafkajs": "^1.15.0",
    "lint-staged": "^11.0.0",
    "mqtt": "^4.2.6",
    "nats": "^2.0.6",
    "nest-commander": "^1.2.0",
    "nest-commander-testing": "^1.1.0",
    "nyc": "^15.1.0",
    "pino": "^6.11.3",
    "prettier": "^2.3.0",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6",
    "socket.io": "^4.1.2",
    "socket.io-client": "^4.1.2",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4",
    "winston": "^3.3.3",
    "ws": "^7.5.0"
  },
  "engines": {
    "node": ">=10"
  },
  "resolutions": {
    "apollo-cache-control": "0.12.0"
  }
}
