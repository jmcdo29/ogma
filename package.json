    "benchmark": "nx affected:bench",
    "commit": "git-cz",
    "coverage": "nx run tools:collect",
    "format": "prettier --write \"{packages,integration,benchmarks,tools}/**/*.ts\"",
    "lint": "eslint --ext .ts .",
    "test": "nx affected:test",
    "test:int": "NODE_NO_WARNINGS=1 nx e2e integration",
    "sendCoverage": "codeclimate-test-reporter < coverage/lcov.info",
    "prepare": "husky install",
    "release": "pnpm changeset publish",
    "nx": "nx",
    "deploy:docs": "nx deploy docs"
