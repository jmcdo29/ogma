    },
    "@ogma/nestjs-module": {
      "optional": false
    }
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.3.1",
    "@nestjs/common": "^7.6.15",
    "@nestjs/core": "^7.6.15",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/testing": "^7.6.15",
    "@ogma/logger": "workspace:*",
    "@ogma/nestjs-module": "workspace:*",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "jest": "^26.6.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.7",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  }
