{
  "name": "@ogma/cli",
  "version": "1.0.0",
  "description": "A CLI to re-hydrate JSON logs back to a dev readable format, colors included.",
  "keywords": [
    "cli",
    "ogma",
    "logs",
    "logger",
    "hydrate",
    "nodejs"
  ],
  "author": "Jay McDoniel <jmcdo29@gmail.com>",
  "homepage": "https://github.com/jmcdo29/ogma",
  "license": "MIT",
  "main": "lib/main.js",
  "types": "lib/index.d.ts",
  "bin": {
    "ogma": "./lib/main.js"
  },
  "files": [
    "lib"
  ],
  "directories": {
    "lib": "lib"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmcdo29/ogma.git"
  },
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "nest build",
    "postbuild": "mv ./lib/src/* ./lib && rmdir lib/src",
    "lint": "eslint --ext .ts ./src/",
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "engines": {
    "node": ">=10"
  },
  "bugs": {
    "url": "https://github.com/jmcdo29/ogma/issues"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.15",
    "@nestjs/core": "^7.6.15",
    "@ogma/logger": "workspace:*",
    "nest-commander": "^1.0.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.7"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/testing": "^7.6.15",
    "jest": "26.4.2",
    "nest-commander-testing": "^1.0.1",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  }
}
