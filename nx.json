{
  "npmScope": "ogma",
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "nx.json": "*"
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "workspaceLayout": {
    "libsDir": "packages"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": [
          "build",
          "test",
          "lint",
          "package",
          "prepare",
          "e2e",
          "e2e-run",
          "docker-up",
          "docker-ready"
        ],
        "strictlyOrderedTargets": ["build", "package", "prepare"],
        "canTrackAnalytics": false,
        "showUsageWarnings": true,
        "accessToken": "ODFlNDgyYzItZTgwYy00OTQyLTkxNTgtOWQ4ZTVlNDU3OTU2fHJlYWQ="
      }
    }
  },
  "affected": {
    "defaultBase": "main"
  }
}
